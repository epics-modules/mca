grecord(ao,"$(P)$(HVPS)VOLT_OUT") {
    field(DESC,"HVPS output voltage")
    field(PINI,"YES")
    field(DRVL,"0")
    field(LOPR,"0")
    field(DRVH,"$(LIMIT)")
    field(HOPR,"$(LIMIT)")
    field(PREC,"0")
    field(VAL,"0")
    field(DTYP, "asyn ICB")
    field(OUT,"#C2 S0 @$(PORT),$(ADDR)")
}
