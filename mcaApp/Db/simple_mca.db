grecord(mca,"$(P)$(M)")
{
    field(DTYP, "$(DTYP)")
    field(PINI, "YES")
    field(NMAX, "$(CHANS)")
    field(NUSE, "$(CHANS)")
    field(PREC, "$(PREC)")
    field(INP,  "$(INP)")
# Alarm setpoints, severity and hysteresis for deadtime
    field(HIHI,"70.")
    field(HHSV,"MAJOR")
    field(HIGH,"40.")
    field(HSV,"MINOR")
    field(LOLO,"0.")
    field(LLSV,"NO_ALARM")
    field(LOW,"0.")
    field(LSV,"NO_ALARM")
    field(HYST,"2.")
}
