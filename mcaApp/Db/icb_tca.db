grecord(mbbo,"$(P)$(TCA)POLARITY") {
    field(DESC,"Output polarity")
    field(PINI,"YES")
    field(ZRVL,"0")
    field(ZRST,"Normal")
    field(ONVL,"1")
    field(ONST,"Inverted")
    field(DTYP, "MPF ICB")
    field(OUT,"#C$(CARD) S1 @$(SERVER),$(ADDR)")
}
