        field(SDIS,"$(P)Acquiring NPP NMS")
        field(VAL, "1")
        field(OUTA,"$(P)mca1.PROC  PP MS")
        field(OUTB,"$(P)mca2.PROC  PP MS")
        field(OUTC,"$(P)mca3.PROC  PP MS")
        field(OUTD,"$(P)mca4.PROC  PP MS")
