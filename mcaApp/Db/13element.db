grecord(dfanout,"$(P)StatusAll1") {
        field(VAL, "1")
        field(OUTA,"$(P)mca9.PROC  PP MS")
        field(OUTB,"$(P)mca10.PROC  PP MS")
        field(OUTC,"$(P)mca11.PROC  PP MS")
        field(OUTD,"$(P)mca12.PROC  PP MS")
